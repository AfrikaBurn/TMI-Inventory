Ext.define('{{ className }}', {
    extend: 'Ext.data.Model',
    alias: 'schema.{{ className }}',

    fields: [
        {% for field in fields %}
        {
            name: '{{ field.name }}',
            type: '{{ field.type }}'
        }{% if not loop.last %},{% endif %}

        {% endfor %}
    ],

    proxy: {
        type: "PartKeeprREST",
        url: '{{ uri|raw }}'
    }
});